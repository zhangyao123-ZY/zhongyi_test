<!--components/custom-tab-bar.wxml-->
<van-tabbar wx:if="{{ show }}" active="{{ active }}" bind:change="onChange">
  <van-tabbar-item wx:for="{{ list }}" wx:key="index">
    <image slot="icon" src="{{ item.icon.normal }}" mode="aspectFit" style="width: 24px; height: 24px;" />
    <image slot="icon-active" src="{{ item.icon.active }}" mode="aspectFit" style="width: 24px; height: 24px;" />
    {{ item.text }}
  </van-tabbar-item>
</van-tabbar>